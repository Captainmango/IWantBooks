<%= form_for @book do |f| %>

  <div class="field">
    <%= f.label :title %><br />
    <%= f.text_field :title, :readonly=> true%>
  </div>

  <div class="field">
    <%= f.label :author %> <br />
    <%= f.text_field :author%>
  </div>

  <div class="field">
    <%= f.label :self_link %><br />
    <%= f.text_field :self_link %>
  </div>
  
  <div class="field">
    <%= f.label :preview_link %><br />
    <%= f.text_field :preview_link %>
  </div>

  <div class="actions">
    <%= f.submit%>
  </div>
<% end %>